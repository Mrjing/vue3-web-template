{
	"$schema": "<https://turbo.build/schema.json>",
	"tasks": {
		"dev": {
			"cache": false
		},
		"build": {
			"dependsOn": ["^build"],
			"outputs": [".next/**", "!.next/cache/**"]
		},
		"test": {
			"dependsOn": ["build"],
			"inputs": ["src/**/*.tsx", "src/**/*.ts", "test/**/*.ts", "test/**/*.tsx"]
		},
		"lint": {}
	}
}
